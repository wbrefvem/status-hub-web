<article>
    <section class="row">
        <ul class="well col-md-offset-3 col-md-6">
            <li><h3>{{model.title}}</h3></li>
            <li>{{model.description}}</li>
            <li><small>{{formattedStartDate}} - {{formattedEndDate}}</small></li>
            <li>{{#link-to 'status.detail.edit' model.id}}edit{{/link-to}}</li>
        </ul>
    </section>
    {{outlet}}
</article>
