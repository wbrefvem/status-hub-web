<article>
    <section>
        <ul class="list-group col-md-offset-3 col-md-6">
            {{#each status in model}}
                <li class="list-group-item pointed-at" {{action 'detail' status}}>{{status.title}} #{{status.id}}</li>
            {{/each}}
        </ul>
    </section>
</article>
{{outlet}}
